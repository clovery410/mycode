#include <stdio.h>

int main (void)
{
  float fahr, celsius;
  int lower, upper, step;

  lower = 0;
  upper = 150;
  step = 10;

  for (celsius = lower; celsius <= upper; celsius += step)
    {
      fahr = (9.0 / 5.0) * celsius + 32.0;
      printf("%3f\t%6.1f\n", celsius, fahr);

    }
  return 0;

}
